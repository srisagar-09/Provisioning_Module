pipeline{
    agent any
    environment{
        a = "Devops Engineer"
        b = "Cloud Engineer"
    }
    stages{
        stage('Career options'){
            environment{
                c = "Java Developer"
            }
            Steps{
                echo "${a}"
                echo "${b}"
                echo "${c}"
            }
        }
        Stage ('Career'){
            steps{
              echo "${a}"
              echo "${b}"  
            }
        }
    }
}
